<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.10.xsd">


    <changeSet author="dorogokupets" id="changelog-schema" runOnChange="true">
        <sql endDelimiter=";">
            CREATE SCHEMA IF NOT EXISTS changelog
        </sql>
    </changeSet>

    <changeSet author="dorogokupets" id="cvservice-schema" runOnChange="true">
        <sql endDelimiter=";">
            CREATE SCHEMA IF NOT EXISTS cvservice
        </sql>
    </changeSet>


</databaseChangeLog>
